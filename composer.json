{
    "name": "gaussian-wonder/quick-laravel-migrations",
    "description": "A package made for importing database dumps faster than the laravel classic migration system.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "GaussianWonder",
            "email": "teo.virghi@yahoo.ro"
        }
    ],
    "keywords": [
        "laravel",
        "laravel migrations",
        "laravel quick migrations",
